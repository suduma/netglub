		:name=>"hash",
		:string=>"ca2ffbc939969a2246cde196f0fc4841"}]],
	 ["HTTPServer",
	  [{:probability=>100, :name=>"server string", :string=>"Apache"}]],
	 ["Header-Hash",
	  [{:probability=>100,
		:name=>"hash",
		:string=>"2df7eaaa4480f28013aaf48ae9266b84"}]],
	 ["MD5",
